<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win+wpa81+wp80+MonoAndroid10+xamarinios10+MonoTouch10</AssetTargetFallback>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/viagogo/gogokit.net</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <Company>viagogo</Company>
    <Copyright>Copyright viagogo 2018</Copyright>
    <Authors>viagogo</Authors>
    <PackageLicenseUrl>https://raw.githubusercontent.com/viagogo/gogokit.net/master/LICENSE.txt</PackageLicenseUrl>
    <Description>GogoKit is a lightweight, async viagogo API client library for .NET.</Description>

    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="HalKit">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json">
      <Version>11.0.2</Version>
    </PackageReference>
    <PackageReference Include="Tavis.UriTemplates">
      <Version>1.1.1</Version>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <Import Project="..\..\version.props" />
</Project>